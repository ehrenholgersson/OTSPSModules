
function Get-SecCredentials{
    param(
        [Parameter(Mandatory)][string]$Name,
        [Parameter(Mandatory)]$Secret
    )
    $dataPath = "$env:ProgramData\OTS\data\1.dat"

    script:$a = Get-Content $dataPath -raw | ConvertFrom-SecureString -AsPlainText
    Unlock-SecretStore -Password $a
    Get-Secret -Name $Name -Secret $Secret
}